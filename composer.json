{
    "name": "michelmelo/laravel-request-logging",
    "description": "Write all incoming requests and there responses",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Michel Melo",
            "email": "eu@michelmelo.pt"
        }
    ],
    "homepage": "https://github.com/michelmelo/laravel-request-logging",
    "keywords": ["Laravel", "request", "logging"],
    "require": {
        "laravel/framework": "~5|~6|~7|~8|~9|~10",
        "illuminate/support": "~5|~6|~7|~8|~9|~10"
    },
    "autoload": {
        "psr-4": {
            "MichelMelo\\RequestLogging\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MichelMelo\\RequestLogging\\": "src/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
         "fixer": [
            "php-cs-fixer fix"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MichelMelo\\RequestLogging\\RequestLoggingServiceProvider"
            ]
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.6",
        "mockery/mockery": "^1.5",
        "orchestra/database": "^6.0",
        "orchestra/testbench": "^6.24",
        "phpunit/phpunit": "^9.5",
        "sempro/phpunit-pretty-print": "^1.4",
        "timacdonald/log-fake": "^1.9"
    }
}
